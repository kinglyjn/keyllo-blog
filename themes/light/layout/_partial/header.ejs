<div class="alignleft">
  <h1><a href="<%- config.root %>"><%= config.title %></a></h1>
  <h2><a href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></h2>
</div>
<nav id="main-nav" class="alignright">
  <ul>
    <% for (var i in theme.menu){ %>
      <% if (i=='首页') { %>
      	<li style="margin-left:0px;"><a href="<%- config.root %><%- theme.menu[i] %>"><%= i %></a></li>
      <% } else { %>
      	<li style="margin-left:25px;"><a href="<%- config.root %><%- theme.menu[i] %>"><%= i %></a></li>
      <% } %>
    <% } %>
    <li style="margin-left:25px;"><a href="http://archive.keyllo.com/">K档</a></li>
    <li style="margin-left:25px;"><a href="/about">关于</a></li>
    <li><a href="javascript:void(0);"> <i id="my-open-search-btn" class="fa fa-search fa-lg md-trigger" data-modal="modal-1"></i> </a></li>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>
